<h1>Repuestos VW Escarabajo</h1>
<ul>
    {{#each products}}
        <li>
            <strong>{{this.title}}</strong><br>
            Descripción: {{this.description}}<br>
            Precio: ${{this.price}}<br>
            Código: {{this.code}}<br>
            Stock: {{this.stock}}
        </li>
    {{/each}}
</ul>